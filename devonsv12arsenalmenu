-- Scripts
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()

-- Click Tp
ClickTpButton.MouseButton1Click:Connect(function()
    ClickTpButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    UserInputService.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            if LocalPlayer.Character then
                LocalPlayer.Character:MoveTo(Mouse.Hit.Position)
            end
        end
    end)
end)

-- Kill All
KillAllButton.MouseButton1Click:Connect(function()
    KillAllButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MisterGunXD/yes/main/rolve%3C3.lua"))()
end)

-- AimBot, SoftAim, HBE, NoClip, TriggerBot scripts would follow similarly
-- Example toggle template:
local function toggleButton(btn, stateTable, key)
    btn.MouseButton1Click:Connect(function()
        stateTable[key] = not stateTable[key]
        btn.BackgroundColor3 = stateTable[key] and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    end)
end

local State = {
    AimBot = false,
    SoftAim = false,
    HBE = false,
    NoClip = false,
    TriggerBot = false
}

toggleButton(AimBotButton, State, "AimBot")
toggleButton(SoftAimButton, State, "SoftAim")
toggleButton(HBEButton, State, "HBE")
toggleButton(NoClipButton, State, "NoClip")
toggleButton(TriggerBotButton, State, "TriggerBot")
